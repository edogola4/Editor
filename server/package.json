{
  "name": "collaborative-editor-server",
  "version": "1.0.0",
  "description": "Collaborative code editor server",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec tsx src/index.ts",
    "migrate": "tsx scripts/run-migrations.js",
    "build": "tsc",
    "generate:types": "tsx scripts/generate-types.ts",
    "test": "vitest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:migrate": "node -r dotenv/config ./node_modules/.bin/node-pg-migrate up --config-file ./db/migrate-config.json",
    "db:rollback": "node -r dotenv/config ./node_modules/.bin/node-pg-migrate down --config-file ./db/migrate-config.json",
    "db:seed": "tsx scripts/seed.ts",
    "db:seed:undo": "NODE_OPTIONS='--loader ts-node/esm' NODE_ENV=development node --no-warnings=ExperimentalWarning scripts/run-seed-undo.js",
    "test:db": "NODE_OPTIONS='--loader ts-node/esm' tsx scripts/test-db.ts"
  },
  "keywords": [
    "collaborative",
    "editor",
    "realtime",
    "socket.io"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@socket.io/redis-adapter": "^8.3.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/ejs": "^3.1.5",
    "@types/express-session": "^1.18.2",
    "@types/ioredis": "^4.28.10",
    "@types/nodemailer": "^6.4.20",
    "@types/sequelize": "^4.28.20",
    "@types/socket.io": "^3.0.1",
    "@types/yamljs": "^0.2.34",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "connect-pg-simple": "^10.0.0",
    "connect-redis": "^8.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.2.0",
    "ioredis": "^5.8.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "node-pg-migrate": "^6.2.2",
    "nodemailer": "^7.0.6",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.2",
    "sequelize-typescript": "^2.1.6",
    "socket.io": "^4.8.1",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "umzug": "^3.8.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.19.19",
    "@types/redis": "^4.0.10",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "ioredis-mock": "^8.13.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "socket.io-client": "^4.8.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  }
}
