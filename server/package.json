{
  "name": "collaborative-editor-server",
  "version": "1.0.0",
  "description": "Real-time collaborative code editor backend",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec tsx src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "migrate": "tsx node_modules/.bin/sequelize-cli db:migrate",
    "migrate:undo": "tsx node_modules/.bin/sequelize-cli db:migrate:undo",
    "seed": "tsx node_modules/.bin/sequelize-cli db:seed:all",
    "db:migrate": "node-pg-migrate up --config-file ./db/migrate-config.json",
    "db:rollback": "node-pg-migrate down --config-file ./db/migrate-config.json"
  },
  "keywords": ["collaborative", "editor", "realtime", "socket.io"],
  "author": "",
  "license": "ISC",
  "_moduleAliases": {
    "@db": "./db"
  },
  "dependencies": {
    "@types/module-alias": "^2.0.4",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.1",
    "nanoid": "^5.1.5",
    "pg-hstore": "^2.3.4",
    "redis": "^5.8.2",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "socket.io": "^4.8.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.5.2",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.1.3",
    "node-pg-migrate": "^8.0.3",
    "nodemon": "^3.1.10",
    "pg": "^8.16.3",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
